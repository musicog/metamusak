@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix oa: <http://www.w3.org/ns/oa#> .


{DigitalSignal1} a mo:DigitalSignal ;
				mo:time {Interval1} ;
				mo:published_as {Record1} .
				
{Interval1} a tl:Interval ;
            tl:start {Interval1startTime} ;
			tl:duration {Interval1Duration} .

{Record1} a mo:Record ;
          mo:MediaType {Record1MediaType} ;
		  rdfs:label "performance audio captured during a specific performance." .

{Record1MediaType} a dc:Mediatype ;
					rdfs:label ".mp3" .

{Record2}  a mo:Record ;
			mo:other_releaseof {Record1} ;
			rdfs:label "substitute audio" .

