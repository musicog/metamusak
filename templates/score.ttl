prefix ma: <http://www.w3.org/ns/ma-ont#> 
prefix dc: <http://purl.org/dc/elements/1.1/> 
prefix mo: <http://purl.org/ontology/mo/> 
prefix event: <http://purl.org/NET/c4dm/event.owl#> 
prefix foaf: <http://xmlns.com/foaf/0.1/> 
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
prefix oa: <http://www.w3.org/ns/oa#> 
prefix tl: <http://purl.org/NET/c4dm/timeline.owl#> 
prefix prov: <http://www.w3.org/TR/prov-o/>


#FIXME move back into performance.ttl for construct:
{musicalWork} mo:arranged_in _:arrangement
_:arrangement a mo:Arrangement ; #FIXME move back here for construct 
			  event:agent {Agent4} ;
              event:product _:conceptScore .
{Agent4} a foaf:Agent ; #FIXME move back here for construct
		 rdfs:label "arranger name" .
_:conceptScore a mo:Score; #FIXME move back here for construct
			   mo:published_as {score} .
#END-FIXME-move-for-construct

{score} a mo:PublishedScore ;
		dc:hasPart {pageOfScore} ;
		mo:publication_of {conceptScore} .
# {conceptScore} defined in {performance}
# 1 score has many pages, each page is associated with one score only	
# each page of the score has exactly one image only

{pageOfScore} mo:mediatype dct:Image ;
			  event:factorsIn {MusicalManifestationRealizationEvent} ;
			  dc:PartOf {score} .
# there are many pages in one score
#one page per {MusicalManifestationRealizationEvent} 
# {MusicalManifestationRealizationEvent} is defined in performancepageturn.ttl
									   				 
# {performance} has its own file that includes {Agent6}
# {performancePageTurns} is te file of performance-based pageturns (Richard)


