@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix oa: <http://www.w3.org/ns/oa#> .

{MusicalWork1} a mo:MusicalWork ;
				mo:composed_in {CompositionEvent} ;
				mo:arranged_in {ArrangementEvent} ;
				rdfs:label "Das Rheingold" ;
				rdf:comment "the opera in the FRBR sense of a Musical Work." .

{CompositionEvent} a mo:Composition ;
					mo:composer {Agent4} .

{ArrangementEvent} a mo:Arrangement ;
				   event:product {Score1} ;
				   event:agent {Agent3} .
				   
{Agent4} a foaf:Agent ;
		rdfs:label "Richard Wagner" .

{Agent3} a foaf:Agent ;
		rdfs:label "Felix Mottl" .
				   
					
				