CONSTRUCT{{
	?annotatorVideo ?aVp ?aVo.
	?annotatorVideoInterval ?aVIp ?aVIo .
}} WHERE {{
	BIND({annotatorVideo} as ?annotatorVideo) .
	?annotatorVideo mo:time ?annotatorVideoInterval ;
		?aVp ?aVo .
	?annotatorVideoInterval ?aVIp ?aVIo .
}}


